<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.state.dao.org.OrgResourcesDaoI">

	<insert id="insert" parameterType="OrgResourcesPo">
		INSERT INTO <include refid='public.model' /> ORG_RESOURCE(ID_,
		PID_,
		NAME_,
		ENABLED_,
		RESOURCE_ID_,
		TYPE_,
		REMARK_) VALUES(
		#{id ,jdbcType=VARCHAR},
		#{PID ,jdbcType=VARCHAR},
		#{name ,jdbcType=VARCHAR},
		#{enabled ,jdbcType=VARCHAR},
		#{resourcesId ,jdbcType=INTEGER},
		#{type, jdbcType=VARCHAR},
		#{remark, jdbcType=VARCHAR}
		)
	</insert>
	
	
	<resultMap id="orgResourcesResultMap" type="OrgResourcesPo">
    <id property="id" column="ID_" jdbcType="VARCHAR" />
    <result property="pid" column="PID_" jdbcType="VARCHAR" />
    <result property="enabled" column="ENABLED_" jdbcType="VARCHAR" />
    <result property="resourceId" column="RESOURCE_ID_" jdbcType="VARCHAR" />
    <result property="remark" column="REMARK_" jdbcType="VARCHAR" />
    <result property="name" column="NAME_" jdbcType="INTEGER" />
    <result property="type" column="TYPE_" jdbcType="INTEGER" />
  </resultMap>
	
	<select id="selectResourcesListByPid" parameterType="string" resultMap="orgResourcesResultMap">
   	select * from <include refid='public.model' /> ORG_RESOURCE where PID_ = #{pid, jdbcType=VARCHAR} and ENABLED_ is null
   	
  </select>
  
</mapper>